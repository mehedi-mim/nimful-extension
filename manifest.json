{
  "manifest_version": 3,
  "name": "Nimful",
  "version": "1.0",
  "description": "Interacts with nimful's wordcloud",
  "icons": {
    "16": "images/real16.png",
    "48": "images/real48.png",
    "128": "images/real128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/real16.png",
      "48": "images/real48.png",
      "128": "images/real128.png"
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://nimful.com"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
